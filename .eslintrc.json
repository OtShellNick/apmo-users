{
    "env": {
        "browser": true,
        "es2021": true,
        "jest/globals": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
    },
    "settings": {
        "import/resolver": {
            "alias": {
                "map": [
                    ["@", "./src"],
                    ["@components", "./src/components"],
                    ["@style", "./src/style"],
                    ["@store", "./src/store"]
                ],
                "extensions": [".js", ".jsx"]
            }
        }
    }
}
